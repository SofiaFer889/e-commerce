{{> header}}
<div>

    <table>
        <head>
            <th>titulo</th>
            <th>precio</th>
            <th>imag</th>
        </head>
    </table>
    <tbody>
        {{#each products}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td>
                {{#if this.thumbnails.length}}
                <a href="{{this.thmbnails.[0]}}" target="_blank">{{this.thmbnails.[0]}}</a>
                {{else}}
                no hay imagen
                {{/if}}
            </td>
        </tr>
        {{/each}}
    </tbody>
</div>